# ~/.tfwmrc

[options]
java_workaround       = 1
move_step             = 30
resize_step           = 30
pixmap_border         = 0
border_width          = 4
; focus_color           = #00cc00
unfocus_color         = slate gray
sloppy_focus          = 0
cursor_position       = 0

[keybinds]
move_up               = Mod1+k
move_down             = Mod1+j
move_left             = Mod1+h
move_right            = Mod1+l
resize_grow_height    = Mod1+Shift+j
resize_grow_width     = Mod1+Shift+l
resize_shrink_height  = Mod1+Shift+k
resize_shrink_width   = Mod1+Shift+h
resize_grow_both      = Mod1+Ctrl+j
resize_shrink_both    = Mod1+Ctrl+k
cycle_next            = Mod1+Tab
cycle_prev            = Mod1+Shift+Tab
teleport_center       = Mod1+s
teleport_top_left     = Mod1+y
teleport_top_right    = Mod1+u
teleport_bottom_left  = Mod1+b
teleport_bottom_right = Mod1+n
fullscreen            = Mod1+x
maximize_vert         = Mod1+m
maximize_horz         = Mod1+Shift+m
kill_win              = Mod1+q
prior_workspace       = Mod1+grave
prev_workspace        = Mod1+bracketleft
next_workspace        = Mod1+bracketright
restart               = Mod1+Shift+r
quit                  = Mod1+Shift+e

terminal1             = Mod1+Return
terminal2             = Mod1+t
browser               = Mod1+w
browser2              = Mod1+e
launcher              = Mod1+space

select_workspace_1    = Mod1+1
select_workspace_2    = Mod1+2
select_workspace_3    = Mod1+3
select_workspace_4    = Mod1+4
select_workspace_5    = Mod1+5
select_workspace_6    = Mod1+6
select_workspace_7    = Mod1+7
select_workspace_8    = Mod1+8
select_workspace_9    = Mod1+9
select_workspace_0    = Mod1+0
send_to_workspace_1   = Mod1+Shift+1
send_to_workspace_2   = Mod1+Shift+2
send_to_workspace_3   = Mod1+Shift+3
send_to_workspace_4   = Mod1+Shift+4
send_to_workspace_5   = Mod1+Shift+5
send_to_workspace_6   = Mod1+Shift+6
send_to_workspace_7   = Mod1+Shift+7
send_to_workspace_8   = Mod1+Shift+8
send_to_workspace_9   = Mod1+Shift+9
send_to_workspace_0   = Mod1+Shift+0

